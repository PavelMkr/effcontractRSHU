<body>
  <div class="content">
    <input class="form-control" type="text" placeholder="Параметры для фильтрации" id="search-text" onkeyup="filter(this, 'sf2')">
    <th colspan="6"><h2 class="mb-5"><center>Отчеты</center></h2></th>
    <table id="sf2" class="centered highlight">
      <thead>
        <tr>
          <td><center>ФИО</center></td>
          <td><center>Должность</center></td>
          <td><center>Кафедра</center></td>
          <td><center>Кол-во непроверенных ЭК / Всего ЭК</center></td>
          <td><center>Непроверенные ЭК</center></td>
        </tr>
      </thead>
      
      {{#each usersData}}
      <tbody>
        <tr>
          <td>{{this.last_name}} {{this.name_real}} {{this.patronymic}}</td>
          <td>{{this.position}}</td>
          <td>{{this.department}}</td>
          <td>
            {{this.not_checked}}/{{ this.all_ek }}
          </td>
          <td><a class="waves-effect waves-light btn" href="/adminUserEK/{{educator_id}}" class="floating-button">Открыть папку</a></td>
        </tr>
      </tbody>  
      {{/each}}
    </table>
  </div>
</body>
</html>
