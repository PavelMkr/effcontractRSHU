<body>
  <div class="content">
    <table id="sf2" class="striped centered">
      <input class="form-control" type="text" placeholder="Параметры для фильтрации" id="search-text" onkeyup="filter(this, 'sf2')">
      <th colspan="6"><h2 class="mb-5"><center>Отчеты</center></h2></th>
      <tr>

        <td>ФИО</td>
        <td>Должность</td>
        <td>Кафедра</td>
        <td>Кол-во непроверенных отчётов / Всего отчётов</td>
        <td>Скачать отчёт</td>
      </tr>
      {{#each usersData}}
        <tr>
          <td>{{this.last_name}} {{this.name_real}} {{this.patronymic}}</td>
          <td>{{this.position}}</td>
          <td>{{this.department}}</td>
          <td>
            {{this.not_checked}}/{{ this.all_ek }}
          </td>
          <td><a href="admin_user_ek/{{educator_id}}" class="floating-button">Открыть папку</a></td>
        </tr>
      {{/each}}
    </table>
  </div>
</body>
</html>
